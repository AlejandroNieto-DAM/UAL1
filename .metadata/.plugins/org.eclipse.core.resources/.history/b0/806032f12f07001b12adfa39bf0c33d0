package org.ip.sesion01;

public class DivisionPorCero {

	public static void main(String[] args) {
		
		float fDividendo = 17.0f;
		float fDivisor = 0.0f;
		
		int iDividendo = 17;
		int iDivisor = 0;
		
		System.out.println(fDividendo + " / " + fDivisor + " = " + fDividendo / fDivisor);
		System.out.println(fDividendo + " % " + fDivisor + " = " + (fDividendo % fDivisor));
		
		System.out.println(iDividendo + " / " + iDivisor + " = ");
		System.out.println(iDividendo/iDivisor);
		
	}

}
