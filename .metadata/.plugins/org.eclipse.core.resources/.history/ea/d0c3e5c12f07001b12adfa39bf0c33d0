package org.ip.sesion01;

public class DivisionPorCero {

	public static void main(String[] args) {
		
		float dividendo = 17.0f;
		float divisor = 0.0f;
		System.out.println(17.0 + " / " + 0.0 + " = " + 17.0/0.0);
		System.out.println(17.0 + " % " + 0.0 + " = " + 17.0%0.0);
		System.out.println(17 + " / " + 0 + " = ");
		System.out.println(17/0);
		
	}

}
