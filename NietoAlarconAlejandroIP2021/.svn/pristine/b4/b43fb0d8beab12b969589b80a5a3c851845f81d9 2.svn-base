package org.ip.sesion02;

public class CodigoDefectuoso {

	public static void main(String[] args) {
		
		int codigoAIntroducir = 25000;
		String isDefectuoso = "";
		
		if ((codigoAIntroducir >= 14681 && codigoAIntroducir <= 15681) || (codigoAIntroducir >= 70001 && codigoAIntroducir <= 79999) || (codigoAIntroducir >= 99999 && codigoAIntroducir <= 110110)){
			isDefectuoso = "DEFECTUOSO";
		} else {
			isDefectuoso = "NO DEFECTUOSO";
		}
		
		System.out.println("El codigo " + codigoAIntroducir + " corresponde a un articulo " + isDefectuoso);

	}

}
