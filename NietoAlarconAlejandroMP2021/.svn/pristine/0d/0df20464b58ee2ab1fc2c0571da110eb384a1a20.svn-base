package org.mp.sesionexamenmayo.p2;

import java.util.Comparator;

public class HabitantesComparator implements Comparator{
	
	@Override
	public int compare(Object o1, Object o2) {
		
		Municipio m1 = (Municipio) o1;
		Municipio m2 = (Municipio) o2;
		

		if (m1.getHabitantes() > m2.getHabitantes())
			return 1;
		else if (m1.getHabitantes() < m2.getHabitantes())
			return -1;
		else
			return 0;
	}
}

	